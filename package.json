{
  "name": "web-skeleton",
  "version": "1.0.0",
  "description": "Project skeleton for a React / Redux web app on top of a Postgres db",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development; npm-run-all --parallel dev:webpack dev:server",
    "dev:webpack": "webpack-dev-server --config config/webpack.js",
    "dev:server": "nodemon --inspect=0.0.0.0:9229 src/server/ --watch src/server/",
    "start": "export NODE_ENV=production npm run build; node src/server",
    "test": "NODE_ENV=test; npm run test:client; npm run test:server",
    "test:client": "NODE_ENV=test mocha --require babel-register 'src/client/**/*.spec.*' --inspect=0.0.0.0:9228",
    "test:server": "NODE_ENV=test mocha 'src/server/**/*.spec.*' --inspect=0.0.0.0:9228",
    "build": "webpack --config config/webpack.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-angular": "^3.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.0",
    "mockery": "^2.1.0",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.4.2",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "isomorphic-fetch": "^2.2.1",
    "pg": "^7.4.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8",
    "style-loader": "^0.20.2",
    "webpack": "^3.10.0"
  }
}
